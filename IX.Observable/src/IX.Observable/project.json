      "imports": [
        "net461",
        "uap10"
      ],
      "dependencies": {
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "NETSTANDARD15" ]
      },
      "imports": [
        "net462"
      ],
      "dependencies": {
        "System.Runtime.InteropServices": "4.1.0"
      }
