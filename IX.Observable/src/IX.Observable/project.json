        "win80",
        "portable-net45+netcore45",
        "portable-net45+netcore45+wpa81"
      ],
      "dependencies": {
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "netstandard1.2": {
      "buildOptions": {
        "define": [ "NETSTANDARD12" ]
      },
      "imports": [
        "net451",
        "net452",
        "win81",
        "wpa81",
        "portable-netcore451+wpa81",
        "portable-net451+netcore451",
        "portable-net451+netcore451+wpa81",
        "monoandroid",
        "monotouch",
        "monomac"
