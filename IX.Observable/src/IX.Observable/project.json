        "net463",
        "dnxcore50",
        "xamarinios",
        "xamarinpsthree",
        "xamarinpsfour",
        "xamarinpsvita",
        "xamarinwatchos",
        "xamarintvos",
        "xamarinxboxthreesixty",
        "xamarinxboxone"
      ],
      "dependencies": {
        "System.Runtime.InteropServices": "4.1.0"
      }
    }
